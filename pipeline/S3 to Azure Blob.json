{
	"name": "S3 to Azure Blob",
	"properties": {
		"activities": [
			{
				"name": "S3 to Azure Blob Copy",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": false
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "MasterAirline",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "master_faa_file",
						"type": "DatasetReference"
					}
				]
			}
		]
	}
}